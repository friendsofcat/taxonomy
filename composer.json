{
  "name": "friendsofcat/taxonomy",
  "description": "Create and manage a hierarchical taxonomy of terms within different vocabularies",
  "license": "MIT",
  "keywords": [
    "taxonomy",
    "laravel"
  ],
  "authors": [
    {
      "name": "Trex Lim",
      "email": "trexlim@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.1",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Trexology\\Taxonomy\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Trexology\\Taxonomy\\TaxonomyServiceProvider"
      ],
      "aliases": {
        "Taxonomy": "Trexology\\Taxonomy\\Facades\\TaxonomyFacade"
      }
    }
  }
}
